{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/src/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/js/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/src/img/$1"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/$1" }],
  "cleanUrls": false,
  "trailingSlash": false
}
