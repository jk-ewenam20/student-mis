{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "/img/$1"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/$1" }],
  "cleanUrls": false,
  "trailingSlash": false,
  "defaultRoute": "/login.html"
}
