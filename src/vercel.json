{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "login.html"
    },
    {
      "src": "^/(?!login\\.html|images/|js/).*",
      "dest": "login.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    }
  ]
}
